jB,GARF,E,IAAA,E,GAAA,C,kBAOW,QAAA,CAAA,Q,GAwBP,EAtBJ,mBAAA,CAKC,OALD,EAKC;AAJC,IAAA,KAAK,EAAA,eAAA,CAAA,CAAC,yBAAD,EAA0B;AAAA,8BACG,IAAA,CAAA,mBAAA,IAAmB;AADtB,KAA1B,CAAA,CAIN;AAFC,IAAA,GAAG,EAAC;AAEL,GALD,EAIG,WAJH,EAIY,CAJZ,CAsBI,E,gBAfJ,mBAAA,CAQE,OARF,EAQE;AAPA,IAAA,IAAI,EAAC,OAOL;AANA,IAAA,KAAK,EAAC,WAMN;AALA,IAAA,IAAI,EAAC,eAKL;AAJA,IAAA,EAAE,EAAC,YAIH;AAHA,IAAA,KAAK,EAAC,IAGN;AAFA,IAAA,YAAY,EAAC,KAEb;;aADS,QAAA,CAAA,Q,GAAQ,M;;AACjB,GARF,E,IAAA,E,GAAA,C,kBAOW,QAAA,CAAA,Q,GAQP,EANJ,mBAAA,CAKC,OALD,EAKC;AAJC,IAAA,KAAK,EAAA,eAAA,CAAA,CAAC,yBAAD,EAA0B;AAAA,8BACG,IAAA,CAAA,mBAAA,IAAmB;AADtB,KAA1B,CAAA,CAIN;AAFC,IAAA,GAAG,EAAC;AAEL,GALD,EAIG,WAJH,EAIY,CAJZ,CAMI,CAhEN,CAiEI,CAnEN,CAoEI,CA7KN,C","sourcesContent":["<template>\r\n  <div class=\"list-group list-group-flush\">\r\n    <div class=\"list-group-item\">\r\n      <div class=\"row justify-content-between g-0 mb-2\">\r\n        <span class=\"col h4 mb-0 py-auto\">Материал</span>\r\n        <span class=\"col text-end fw-bold pb-0\" v-if=\"materialSum.result != 0\"\r\n          >{{\r\n            materialSum.result + measurementPrice.result + overPrice.result\r\n          }}&nbsp;руб.</span\r\n        >\r\n      </div>\r\n      <ul class=\"res-list\">\r\n        <li\r\n          v-for=\"(material, index) of materials\"\r\n          :key=\"index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col h6 my-auto\" v-if=\"material.name\">{{\r\n            material.name\r\n          }}</span>\r\n          <span class=\"col my-auto\" v-else>Кварцевый агломерат</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{\r\n              material.result + overPrice.result / materials.length\r\n            }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n\r\n        <li\r\n          class=\"row justify-content-between g-0 mt-2\"\r\n          v-if=\"measurementPrice.result > 0\"\r\n        >\r\n          <span class=\"col my-auto\">Материалы / расходники</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{ measurementPrice.result }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"list-group-item\" v-if=\"optionSum.length\">\r\n      <div class=\"row justify-content-between g-0 mb-2\">\r\n        <span class=\"col h4 mb-0 py-auto\">Изделие</span>\r\n        <span class=\"col text-end fw-bold pb-0\" v-if=\"totalProductSum.discount\"\r\n          >{{ totalProductSum.discount }}&nbsp;руб.</span\r\n        >\r\n      </div>\r\n      <ul class=\"res-list\">\r\n        <li\r\n          v-for=\"(product, product_index) of products\"\r\n          :key=\"product_index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col my-auto\">{{ product.name }}</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{ productValue(product_index) }}&nbsp;руб.</span\r\n          >\r\n          <ul class=\"col-12 res-list\">\r\n            <li\r\n              class=\"small-text row justify-content-between g-0 mb-2\"\r\n              v-for=\"(option, option_index) of product.options\"\r\n              :key=\"option_index\"\r\n            >\r\n              <span class=\"col my-auto\">{{ option.text }}</span>\r\n              <span class=\"col text-end my-auto\"\r\n                >{{ option.discount }}&nbsp;руб.</span\r\n              >\r\n            </li>\r\n            <li\r\n              class=\"small-text row justify-content-between g-0 mb-2\"\r\n              v-for=\"(addon, addon_index) of addons[product_index]\"\r\n              :key=\"addon_index\"\r\n            >\r\n              <span class=\"col my-auto\">{{ addon.name }}</span>\r\n              <span class=\"col text-end my-auto\"\r\n                >{{ addon.discount }}&nbsp;руб.</span\r\n              >\r\n            </li>\r\n          </ul>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"list-group-item\" v-if=\"logisticsList.length\">\r\n      <h4>Логистика</h4>\r\n      <ul class=\"res-list\">\r\n        <li\r\n          v-for=\"(logistic, index) of logisticsList\"\r\n          :key=\"index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col my-auto\">{{ logistic.text }}</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{ logistic.result }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n\r\n        <li\r\n          class=\"row justify-content-between g-0 mt-2\"\r\n          v-if=\"logisticSum.result != 0\"\r\n        >\r\n          <span class=\"col my-auto fst-italic\">Подытог:</span>\r\n          <span class=\"col text-end my-auto fw-bold\"\r\n            >{{ logisticSum.result }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"list-group-item\">\r\n      <h4>Итог: {{ total }}</h4>\r\n      <div class=\"btn-group w-100\" role=\"group\">\r\n        <input\r\n          type=\"radio\"\r\n          class=\"btn-check\"\r\n          name=\"discountRadio\"\r\n          id=\"discount0\"\r\n          value=\"0\"\r\n          autocomplete=\"off\"\r\n          v-model=\"discount\"\r\n        />\r\n        <label\r\n          class=\"btn btn-outline-primary\"\r\n          :class=\"{ 'recommended-discount': recommendedDiscount == 0 }\"\r\n          for=\"discount0\"\r\n          >Без скидки</label\r\n        >\r\n\r\n        <input\r\n          type=\"radio\"\r\n          class=\"btn-check\"\r\n          name=\"discountRadio\"\r\n          id=\"discount10\"\r\n          value=\"10\"\r\n          autocomplete=\"off\"\r\n          v-model=\"discount\"\r\n        />\r\n        <label\r\n          class=\"btn btn-outline-primary\"\r\n          :class=\"{ 'recommended-discount': recommendedDiscount == 10 }\"\r\n          for=\"discount10\"\r\n          >Колонка 1</label\r\n        >\r\n\r\n        <input\r\n          type=\"radio\"\r\n          class=\"btn-check\"\r\n          name=\"discountRadio\"\r\n          id=\"discount20\"\r\n          value=\"20\"\r\n          autocomplete=\"off\"\r\n          v-model=\"discount\"\r\n        />\r\n        <label\r\n          class=\"btn btn-outline-primary\"\r\n          :class=\"{ 'recommended-discount': recommendedDiscount == 20 }\"\r\n          for=\"discount20\"\r\n          >Колонка 2</label\r\n        >\r\n\r\n        <input\r\n          type=\"radio\"\r\n          class=\"btn-check\"\r\n          name=\"discountRadio\"\r\n          id=\"discount30\"\r\n          value=\"30\"\r\n          autocomplete=\"off\"\r\n          v-model=\"discount\"\r\n        />\r\n        <label\r\n          class=\"btn btn-outline-primary\"\r\n          :class=\"{ 'recommended-discount': recommendedDiscount == 30 }\"\r\n          for=\"discount30\"\r\n          >Колонка 3</label\r\n        >\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\nexport default {\r\n  computed: {\r\n    ...mapGetters([\r\n      \"materialSum\",\r\n      \"optionSum\",\r\n      \"productSum\",\r\n      \"additionalWork\",\r\n      \"templates\",\r\n      \"logisticsList\",\r\n      \"logisticRawSum\",\r\n      \"logisticSum\",\r\n      \"addonSumList\",\r\n      \"addonSum\",\r\n      \"rawTotal\",\r\n      \"total\",\r\n      \"spendings\",\r\n      \"recommendedDiscount\",\r\n      \"measurementPrice\",\r\n      \"overPrice\",\r\n      \"products\",\r\n      \"addons\",\r\n      \"materials\",\r\n      \"totalProductSum\",\r\n    ]),\r\n    discount: {\r\n      get() {\r\n        return typeof this.$store.state.values.discount == \"undefined\"\r\n          ? 0\r\n          : this.$store.state.values.discount;\r\n      },\r\n      set(value) {\r\n        this.setDiscount(value);\r\n      },\r\n    },\r\n    productValue(product_index) {\r\n      return (\r\n        this.optionSum[product_index].discount +\r\n        this.addonSumList[product_index].discount\r\n      );\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    setDiscount(value) {\r\n      this.$store.commit(\"setDiscount\", value);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.res-list {\r\n  padding-left: 1rem !important;\r\n}\r\n</style>"],"sourceRoot":""}]}