A,CAAA,cAAA,GAAc,QAAd,GAAc,MAOrB;AANC,IAAA,KAAK,EAAE,KAAA,CAAA,kBAMR;AALC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,QAAA,CAAS,aAAT,GAAyB,MAAM,CAAC,MAAP,CAAc,KAAzC;AAAA,KAAA,CAKN;AAJA,IAAA,KAAK,EAAC,cAIN;AAHA,IAAA,WAAW,EAAC,cAGZ;AAFC,IAAA,MAAI,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,aAAA,IAAA,QAAA,CAAA,aAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAEL;AADC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,YAAA,IAAA,QAAA,CAAA,YAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AACN,GATF,E,IAAA,E,EAAA,E,WAAA,CAWI,EADJ,mBAAA,CAAuD,OAAvD,EAAuD;AAA/C,IAAA,GAAG,EAAA,eAAiB,MAAA,CAAA;AAA2B,GAAvD,EAAmC,cAAnC,EAA+C,CAA/C,EAA+C,WAA/C,CACI,CAZN,CAaI,CAtCN,CAuCI,CAxCN,CAkDI,EATJ,mBAAA,CAQM,KARN,EAAA,WAAA,EAQM,CAPJ,mBAAA,CAMS,QANT,EAMS;AALP,IAAA,IAAI,EAAC,QAKE;AAJP,IAAA,KAAK,EAAC,yCAIC;AAHN,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,UAAA,IAAA,QAAA,CAAA,UAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAGC,GANT,EAIC,KAJD,CAOI,CARN,CASI,CAnDN,CAoDI,CArDN,C","sourcesContent":["<template>\r\n  <div class=\"material-card card mt-5 mt-md-3\">\r\n    <div class=\"material card-body row g-3\">\r\n      <div class=\"col-12 col-sm-11 container\">\r\n        <div class=\"row row-cols-2 g-2\">\r\n          <div class=\"col-12 form-floating\">\r\n            <input\r\n              :id=\"'materialname-' + index\"\r\n              :value=\"cardData.materialName\"\r\n              @input=\"updateStorage('materialName', $event.target.value)\"\r\n              type=\"text\"\r\n              class=\"form-control border-info\"\r\n              placeholder=\"название материала\"\r\n            />\r\n            <label :for=\"'materialname-' + index\">название материала</label>\r\n          </div>\r\n          <div class=\"col-12 col-sm-8 form-floating\">\r\n            <input\r\n              :id=\"'materialprice-' + index\"\r\n              :type=\"isFocused ? 'number' : 'text'\"\r\n              :value=\"formattedSlabPrice\"\r\n              @input=\"cardData.materialPrice = $event.target.value\"\r\n              class=\"form-control\"\r\n              placeholder=\"цена слэба\"\r\n              @blur=\"focusOut\"\r\n              @focus=\"focusIn\"\r\n            />\r\n            <label :for=\"'materialprice-' + index\">цена слэба</label>\r\n          </div>\r\n          <div class=\"col-12 col-sm-4 form-floating\">\r\n            <input\r\n              :id=\"'slabcount-' + index\"\r\n              :type=\"countIsFocused ? 'number' : 'text'\"\r\n              :value=\"formattedSlabCount\"\r\n              @input=\"cardData.materialCount = $event.target.value\"\r\n              class=\"form-control\"\r\n              placeholder=\"число слэбов\"\r\n              @blur=\"focusOutCount\"\r\n              @focus=\"focusInCount\"\r\n            />\r\n            <label :for=\"'slabcount-' + index\">число слэбов</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-12 col-sm-1\">\r\n        <button\r\n          type=\"button\"\r\n          class=\"btn btn-danger h-100 w-100 py-0 px-md-0\"\r\n          @click=\"deleteCard\"\r\n        >\r\n          -\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: \"MaterialCard\",\r\n  props: [\"index\"],\r\n  emits: [\"remove\"],\r\n  data() {\r\n    return {\r\n      capitalize(str) {\r\n        return str\r\n          .split(\" \")\r\n          .map((word) => word.charAt(0).toUpperCase() + word.slice(1))\r\n          .join(\"\");\r\n      },\r\n      isFocused: false,\r\n      countIsFocused: false,\r\n      formattedSlabPrice: this.$store.state.material_cards[this.index]\r\n        .materialPrice\r\n        ? this.$store.state.material_cards[this.index].materialPrice\r\n            .toString()\r\n            .replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1 \") + \" руб/шт.\"\r\n        : null,\r\n      formattedSlabCount: this.$store.state.material_cards[this.index]\r\n        .materialCount\r\n        ? this.$store.state.material_cards[this.index].materialCount + \" шт.\"\r\n        : null,\r\n    };\r\n  },\r\n  computed: {\r\n    cardData() {\r\n      return this.$store.state.material_cards[this.index];\r\n    },\r\n  },\r\n  methods: {\r\n    updateStorage(field, value){\r\n      this.$store.commit('updateMaterial', {index: this.index, field, value})\r\n    },\r\n\r\n\r\n    deleteCard() {\r\n      this.$store.commit(\"deleteMaterialCard\", this.index);\r\n    },\r\n    focusOut: function () {\r\n      this.isFocused = false;\r\n      this.formattedSlabPrice =\r\n        this.cardData.materialPrice\r\n          .toString()\r\n          .replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1 \") + \" руб/шт.\";\r\n      console.log(this.formattedSlabPrice);\r\n      if (this.formattedSlabPrice == \" руб/шт.\") {\r\n        this.formattedSlabPrice = null;\r\n      }\r\n    },\r\n    focusIn: function () {\r\n      this.isFocused = true;\r\n      this.formattedSlabPrice = this.cardData.materialPrice;\r\n    },\r\n    focusOutCount: function () {\r\n      this.countIsFocused = false;\r\n\r\n      this.formattedSlabCount =\r\n        this.cardData.materialCount\r\n          .toString()\r\n          .replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1 \") + \" шт.\";\r\n    },\r\n    focusInCount: function () {\r\n      this.countIsFocused = true;\r\n      this.formattedSlabCount = this.cardData.materialCount;\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}