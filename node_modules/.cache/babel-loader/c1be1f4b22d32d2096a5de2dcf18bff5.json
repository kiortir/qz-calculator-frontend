r\n        <div class=\"row g-2\">\r\n            <div class=\"col-1 form-check\" v-if=\"(typeof addon.isChecked !== 'undefined')\">\r\n                <input class=\"form-check-input\" type=\"checkbox\" v-model=\"addon.isChecked\" />\r\n            </div>\r\n            <div class=\"col h6 my-auto\"> {{ addon.name }}</div>\r\n\r\n            <div class=\"col\" v-if=\"(typeof addon.count !== 'undefined')\">\r\n                <input type=\"number\" class=\"form-control form-control-sm\" :placeholder=\"addon.measurement\" v-model.number=\"addon.count\" />\r\n            </div>\r\n            <div class=\"col-12\" v-if=\"(typeof addon.type !== 'undefined')\">\r\n                <select class=\"form-select form-select-sm\" v-model=\"addon.type.selected\">\r\n                    <option value=\"null\">тип</option>\r\n                    <option v-for=\"selector in addon.type.selectors\" :key=\"selector\"> {{ selector }} </option>\r\n                </select>\r\n            </div>\r\n\r\n        </div>\r\n    </li>\r\n</ul>\r\n</template>\r\n\r\n<script>\r\nlet defaultAddons = [{\r\n        name: \"Радиусный угол > 12\",\r\n        count: null,\r\n        measurement: \"шт.\"\r\n    },\r\n    {\r\n        name: \"Другие отверстия\",\r\n        count: null,\r\n        measurement: \"шт.\"\r\n    },\r\n    {\r\n        name: \"Обход пиластры\",\r\n        count: null,\r\n        measurement: \"шт.\"\r\n    },\r\n    {\r\n        name: \"Конвекция\",\r\n        type: {\r\n            selectors: [\"вровень\", \"фрезеровка\"],\r\n            selected: null\r\n        },\r\n        count: null,\r\n        measurement: \"шт.\",\r\n    },\r\n    {\r\n        name: \"Лучи под сток воды\",\r\n        count: null,\r\n        measurement: \"шт.\"\r\n    },\r\n    {\r\n        name: \"Капельник\",\r\n        count: null,\r\n        measurement: \"шт.\"\r\n    },\r\n    {\r\n        name: \"Монтаж хром опоры заказчика\",\r\n        count: null,\r\n        measurement: \"шт.\"\r\n    },\r\n    {\r\n        name: \"Стыковка опорной ноги из камня под 45\",\r\n        count: null,\r\n        measurement: \"шт.\",\r\n    },\r\n    {\r\n        name: \"Шлифовка подворота камня\",\r\n        type: {\r\n            selectors: [\"до 150 мм\", \"более 150мм\"],\r\n            selected: null\r\n        },\r\n        count: null,\r\n        measurement: \"шт./м2\",\r\n    },\r\n    {\r\n        name: \"Подворот из камня\",\r\n        count: null,\r\n        measurement: \"шт.\"\r\n    },\r\n    {\r\n        name: \"Демонтаж старых изделий (до 6 п.м.)\",\r\n        isChecked: false\r\n    },\r\n];\r\nexport default {\r\n    name: \"lowPriority\",\r\n    props: {\r\n        cardIndex: Number,\r\n    },\r\n    created() {\r\n        if (\r\n            typeof this.$store.state.product_cards[this.cardIndex].addons ==\r\n            \"undefined\"\r\n        ) {\r\n            this.$store.state.product_cards[this.cardIndex].addons = JSON.parse(\r\n                JSON.stringify(defaultAddons)\r\n            );\r\n        }\r\n    },\r\n    computed: {\r\n        sharedData() {\r\n            return this.$store.state.product_cards[this.cardIndex].addons;\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.backlist {\r\n    background-color: cadetblue;\r\n    border-radius: 0.25rem !important;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}