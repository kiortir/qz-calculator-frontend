CAON;AANA,IAAA,IAAI,EAAC,QAML;AALA,IAAA,KAAK,EAAA,eAAA,CAAA,CAAC,cAAD,EAAe;AAAA,uBAIO,QAAA,CAAA,QAAA,CAAS,IAAT,CAAc,QAAd,IAAsB;AAJ7B,KAAf,CAAA,CAKL;AAJA,IAAA,EAAE,EAAC,0BAIH;AAHA,IAAA,WAAW,EAAC,KAGZ;;aAFgB,QAAA,CAAA,QAAA,CAAS,IAAT,CAAc,Q,GAAQ,M;;AAEtC,GARF,E,IAAA,E,EAAA,C,iBAMkB,QAAA,CAAA,QAAA,CAAS,IAAT,CAAc,Q;YAA9B;MAGE,CAVN,CAWI,CA5BN,CA6BI,CA5CN,C","sourcesContent":["<template>\r\n  <div class=\"card\">\r\n    <div class=\"card-header bg-transparent\">\r\n      <div class=\"row g-0\">\r\n        <div class=\"col-10 h-100 my-auto\">Мойка</div>\r\n        <div class=\"col-2\">\r\n          <button\r\n            class=\"btn btn-outline-danger w-100 p-0\"\r\n            type=\"button\"\r\n            @click=\"$emit('removeoption')\"\r\n          >\r\n            -\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"card-body row\">\r\n      <div class=\"col-12 col-xl-8 m-xl-auto mb-1\">\r\n        <select\r\n          class=\"form-select\"\r\n          aria-label=\".form-select-xl example\"\r\n          v-model=\"sinkData.data.type\"\r\n          @change=\"update\"\r\n          :class=\"{ 'border-danger': sinkData.data.type == null }\"\r\n        >\r\n          <option disabled value=\"null\">Тип мойки</option>\r\n          <option>Накладная</option>\r\n          <option>Поджимная</option>\r\n          <option>Интегрированная</option>\r\n          <option>Интегрированная 45°</option>\r\n          <option>Вровень</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"sink-quantity col-12 col-xl-4 m-xl-auto mb-1\">\r\n        <input\r\n          @input=\"update\"\r\n          type=\"number\"\r\n          class=\"form-control\"\r\n          id=\"exampleFormControlInput1\"\r\n          placeholder=\"шт.\"\r\n          v-model.number=\"sinkData.data.quantity\"\r\n          :class=\"{ 'border-danger': sinkData.data.quantity == null }\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  emits: [\"removeoption\", \"refresh\"],\r\n  name: \"sink\",\r\n  props: {\r\n    index: Number,\r\n    cardId: Number,\r\n    compData: Object,\r\n  },\r\n  computed: {\r\n    sinkData() {\r\n      return this.compData;\r\n    },\r\n  },\r\n  methods: {\r\n    update() {\r\n      let data = this.sinkData.data;\r\n      let pricing = this.$store.state.currentPriceList.sink;\r\n      try {\r\n        this.sinkData.result = {\r\n          price: pricing.type.finalPrice(data.type) * data.quantity,\r\n        };\r\n        this.sinkData.salary = {\r\n          price: pricing.type[data.type].price * data.quantity,\r\n        };\r\n        this.sinkData.consumables = 0;\r\n        this.sinkData.consumablesRaw = 0;\r\n      } catch (e) {\r\n        console.log(e);\r\n      }\r\n      this.sinkData.name =\r\n        `Тип мойки: ${data.type}, ${data.quantity} шт.`.toLowerCase();\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}