etween g-0 mb-2\">\r\n        <span class=\"col h4 mb-0 py-auto\">Изделие</span>\r\n        <span class=\"col text-end fw-bold pb-0\" v-if=\"totalProductSum.salary\"\r\n          >{{ totalProductSum.salary }}&nbsp;руб.</span\r\n        >\r\n      </div>\r\n      <ul class=\"res-list\">\r\n        <li\r\n          v-for=\"(product, product_index) of products\"\r\n          :key=\"product_index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col my-auto\">{{ product.name }}</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{\r\n              optionSum[product_index].salary +\r\n              addonSumList[product_index].salary\r\n            }}&nbsp;руб.</span\r\n          >\r\n          <ul class=\"col-12 res-list\">\r\n            <li\r\n              class=\"small-text row justify-content-between g-0 mb-2\"\r\n              v-for=\"(option, option_index) of product.options\"\r\n              :key=\"option_index\"\r\n            >\r\n              <span class=\"col my-auto\">{{ option.text }}</span>\r\n              <span class=\"col text-end my-auto\"\r\n                >{{ option.salary }}&nbsp;руб.</span\r\n              >\r\n            </li>\r\n            <li\r\n              class=\"small-text row justify-content-between g-0 mb-2\"\r\n              v-for=\"(addon, addon_index) of addons[product_index]\"\r\n              :key=\"addon_index\"\r\n            >\r\n              <span class=\"col my-auto\">{{ addon.name }}</span>\r\n              <span class=\"col text-end my-auto\"\r\n                >{{ addon.salary }}&nbsp;руб.</span\r\n              >\r\n            </li>\r\n          </ul>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"list-group-item\" v-if=\"logisticsList.length\">\r\n      <h4>Логистика</h4>\r\n      <ul class=\"res-list\">\r\n        <li\r\n          v-for=\"(logistic, index) of logisticsList\"\r\n          :key=\"index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col my-auto\">{{ logistic.text }}</span>\r\n          <span class=\"col text-end my-auto\" v-if=\"logistic.salary\"\r\n            >{{ logistic.salary }}&nbsp;руб.</span\r\n          >\r\n          <span class=\"col text-end my-auto\" v-else>--</span>\r\n        </li>\r\n        <li\r\n          class=\"row justify-content-between g-0 mt-2\"\r\n          v-if=\"measurementPrice.salary > 0\"\r\n        >\r\n          <span class=\"col my-auto\">Замер</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{ measurementPrice.salary }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n        <li\r\n          class=\"row justify-content-between g-0 mt-2\"\r\n          v-if=\"logisticRawSum.salary != 0\"\r\n        >\r\n          <span class=\"col my-auto fst-italic\">Подытог:</span>\r\n          <span class=\"col text-end my-auto fw-bold\"\r\n            >{{ logisticRawSum.salary }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"list-group-item\">\r\n      <h4>Итог: {{ rawTotal.salary }}</h4>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from \"vuex\";\r\nexport default {\r\n  computed: {\r\n    ...mapGetters([\r\n      \"materialSum\",\r\n      \"optionSum\",\r\n      \"productSum\",\r\n      \"additionalWork\",\r\n      \"templates\",\r\n      \"logisticsList\",\r\n      \"logisticRawSum\",\r\n      \"logisticSum\",\r\n      \"addonSumList\",\r\n      \"addonSum\",\r\n      \"rawTotal\",\r\n      \"total\",\r\n      \"spendings\",\r\n      \"recommendedsalary\",\r\n      \"measurementPrice\",\r\n      \"overPrice\",\r\n      \"products\",\r\n      \"addons\",\r\n      \"materials\",\r\n      \"totalProductSum\",\r\n    ]),\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.res-list {\r\n  padding-left: 1rem !important;\r\n}\r\n</style>"],"sourceRoot":""}]}