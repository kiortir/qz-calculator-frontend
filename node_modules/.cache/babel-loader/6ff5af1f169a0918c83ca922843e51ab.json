mplate>\r\n\r\n<script>\r\nexport default {\r\n  emits: [\"removeoption\", \"refresh\"],\r\n  name: \"ladder\",\r\n  props: {\r\n    optionIndex: Number,\r\n    cardIndex: Number,\r\n  },\r\n  computed: {\r\n    data() {\r\n      return this.$store.state.values.product_cards[this.cardIndex].option_card[\r\n        this.optionIndex\r\n      ].data;\r\n    },\r\n  },\r\n  methods: {\r\n    update(field, value) {\r\n      this.$emit(\"refresh\", this.optionIndex, field, value);\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}