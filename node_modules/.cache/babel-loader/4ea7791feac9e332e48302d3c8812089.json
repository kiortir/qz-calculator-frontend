AA,E,WAAA,CASI,EADJ,mBAAA,CAAiE,OAAjE,EAAiE;AAAzD,IAAA,GAAG,EAAA,oBAAsB,MAAA,CAAA;AAAgC,GAAjE,EAA6C,cAA7C,EAAyD,CAAzD,EAAyD,WAAzD,CACI,CAVN,CAWI,CAzCN,CA0CI,CA3CN,CAqDI,EATJ,mBAAA,CAQM,KARN,EAAA,WAAA,EAQM,CAPJ,mBAAA,CAMS,QANT,EAMS;AALP,IAAA,IAAI,EAAC,QAKE;AAJP,IAAA,KAAK,EAAC,yCAIC;AAHN,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,KAAA,CAAK,QAAL,CAAF;AAAA,KAAA;AAGC,GANT,EAIC,KAJD,CAOI,CARN,CASI,CAtDN,CAuDI,CAxDN,C","sourcesContent":["<template>\r\n  <div class=\"material-card card mt-5 mt-md-3\">\r\n    <div class=\"material card-body row g-3\">\r\n      <div class=\"col-12 col-sm-11 container\">\r\n        <div class=\"row row-cols-2 g-2\">\r\n          <div class=\"col-12 form-floating\">\r\n            <input\r\n              :id=\"'materialname-' + card_index\"\r\n              :value=\"data.materialName\"\r\n              @input=\"\r\n                updateMaterialName({\r\n                  index: card_index,\r\n                  value: $event.target.value,\r\n                })\r\n              \"\r\n              type=\"text\"\r\n              class=\"form-control border-info\"\r\n              placeholder=\"название материала\"\r\n            />\r\n            <label :for=\"'materialname-' + card_index\"\r\n              >название материала</label\r\n            >\r\n          </div>\r\n          <div class=\"col-12 col-sm-8 form-floating\">\r\n            <input\r\n              :id=\"'materialprice-' + card_index\"\r\n              :value=\"data.materialPrice\"\r\n              @input=\"recalculate('materialPrice', $event.target.value)\"\r\n              type=\"number\"\r\n              class=\"form-control\"\r\n              placeholder=\"цена слэба\"\r\n            />\r\n            <label :for=\"'materialprice-' + card_index\">цена слэба</label>\r\n          </div>\r\n          <div class=\"col-12 col-sm-4 form-floating\">\r\n            <input\r\n              :id=\"'material-count-' + card_index\"\r\n              :value=\"data.materialCount\"\r\n              @input=\"recalculate('materialCount', $event.target.value)\"\r\n              type=\"number\"\r\n              class=\"form-control\"\r\n              placeholder=\"число слэбов\"\r\n            />\r\n            <label :for=\"'material-count-' + card_index\">число слэбов</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-12 col-sm-1\">\r\n        <button\r\n          type=\"button\"\r\n          class=\"btn btn-danger h-100 w-100 py-0 px-md-0\"\r\n          @click=\"$emit('remove')\"\r\n        >\r\n          -\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapMutations } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"MaterialCard\",\r\n  props: {\r\n    card_index: Number,\r\n  },\r\n  emits: [\"remove\"],\r\n  data() {\r\n    return {\r\n      priceList: {\r\n        delivery: {\r\n          raw: 3300,\r\n          mul: 1.25,\r\n          price() {\r\n            return this.raw * this.mul;\r\n          },\r\n        },\r\n      },\r\n    };\r\n  },\r\n  computed: {\r\n    data() {\r\n      return this.$store.state.values.material_cards[this.card_index];\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapMutations([\"updateMaterialName\", \"updateMaterialField\"]),\r\n    recalculate(field, value) {\r\n      this.updateMaterialField({\r\n        index: this.card_index,\r\n        value,\r\n        field,\r\n      });\r\n\r\n      let price =\r\n        Number(this.data.materialCount) * Number(this.data.materialPrice) +\r\n        (Number(this.data.materialCount) % 1 == 0)\r\n          ? 0\r\n          : 1500 + this.priceList.delivery.price;\r\n      console.log(price);\r\n      this.updateMaterialField({\r\n        index: this.card_index,\r\n        value: price,\r\n        field: \"result\",\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}