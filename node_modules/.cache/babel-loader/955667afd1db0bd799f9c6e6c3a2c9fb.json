ntrol\"\r\n              placeholder=\"число слэбов\"\r\n            />\r\n            <label :for=\"'material-count-' + index\">число слэбов</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-12 col-sm-1\">\r\n        <button\r\n          type=\"button\"\r\n          class=\"btn btn-danger h-100 w-100 py-0 px-md-0\"\r\n          @click=\"$emit('remove')\"\r\n        >\r\n          -\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: \"MaterialCard\",\r\n  props: {\r\n    card_index: Number,\r\n  },\r\n  emits: [\"remove\"],\r\n  // data() {\r\n  //   return {\r\n  //     capitalize(str) {\r\n  //       return str\r\n  //         .split(\" \")\r\n  //         .map((word) => word.charAt(0).toUpperCase() + word.slice(1))\r\n  //         .join(\"\");\r\n  //     },\r\n  //     isFocused: false,\r\n  //     countIsFocused: false,\r\n  //     formattedSlabPrice: this.$store.state.material_cards[this.index]\r\n  //       .materialPrice\r\n  //       ? this.$store.state.material_cards[this.index].materialPrice\r\n  //           .toString()\r\n  //           .replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1 \") + \" руб/шт.\"\r\n  //       : null,\r\n  //     formattedSlabCount: this.$store.state.material_cards[this.index]\r\n  //       .materialCount\r\n  //       ? this.$store.state.material_cards[this.index].materialCount + \" шт.\"\r\n  //       : null,\r\n  //   };\r\n  // },\r\n  computed: {\r\n    cardData() {\r\n      return this.$store.state.values.material_cards[this.index];\r\n    },\r\n  },\r\n  methods: {\r\n    updateStorage(field, value) {\r\n      this.$store.commit(\"updateMaterial\", { index: this.index, field, value });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}