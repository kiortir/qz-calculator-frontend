{cardIndex, choice: $event.target.value})\"\r\n              >\r\n                <option disabled value=\"null\">Тип изделия</option>\r\n                <option\r\n                  v-for=\"product in Object.keys(PRODUCTS)\"\r\n                  :key=\"product\"\r\n                >{{ product }}</option>\r\n              </select>\r\n            </div>\r\n            <div class=\"col align-self-end h-100\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-danger w-100 btn-lg px-0\"\r\n                @click=\"$emit('remove')\"\r\n              >\r\n                -\r\n              </button>\r\n            </div>\r\n          </div>\r\n          <div class=\"row-cols-md-2 row-cols-1 row gx-3 gy-2\">\r\n            <div\r\n              class=\"col\"\r\n              v-for=\"(option, option_index) in productCard.option_card\"\r\n              :key=\"option_index + cardIndex\"\r\n            >\r\n              <component\r\n                class=\"h-100\"\r\n                :is=\"option.component_type\"\r\n                :index=\"option_index\"\r\n                :key=\"option_index + cardIndex.toString()\"\r\n                :cardId=\"cardIndex\"\r\n                :compData=\"option\"\r\n                @removeoption=\"removeOption(option_index)\"\r\n              ></component>\r\n            </div>\r\n          </div>\r\n          <div v-if=\"productCard.chosenType != null\" class=\"row mt-4 g-1\">\r\n            <div class=\"col-10\">\r\n              <select\r\n                class=\"form-select\"\r\n                aria-label=\".form-select-sm example\"\r\n                v-model=\"selectedAddOn\"\r\n              >\r\n                <option value=\"null\" disabled>Выберите опцию</option>\r\n                <option\r\n                  v-for=\"option in Object.keys(OPTIONS)\"\r\n                  :key=\"option\"\r\n                >{{ option }}</option>\r\n              </select>\r\n            </div>\r\n            <div class=\"col-2\">\r\n              <button\r\n                @click=\"addCardOption({choice: selectedAddOn, cardIndex})\"\r\n                class=\"btn btn-primary w-100 h-100 p-0\"\r\n              >\r\n                +\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div v-else class=\"card back mt-5 mt-md-3\" :ref=\"'cardback' + index\">\r\n        <div class=\"product card-body\">\r\n          <div class=\"row mb-3 g-1\">\r\n            <button\r\n              type=\"button\"\r\n              class=\"\r\n                btn btn-outline-secondary btn-lg\r\n                col-2 col-lg-1\r\n                p-0\r\n                d-none d-md-block\r\n              \"\r\n              @click=\"flip\"\r\n            >\r\n              <svg\r\n                xmlns=\"http://www.w3.org/2000/svg\"\r\n                width=\"16\"\r\n                height=\"16\"\r\n                fill=\"currentColor\"\r\n                class=\"bi bi-arrow-repeat\"\r\n                viewBox=\"0 0 16 16\"\r\n              >\r\n                <path\r\n                  d=\"M11.534 7h3.932a.25.25 0 0 1 .192.41l-1.966 2.36a.25.25 0 0 1-.384 0l-1.966-2.36a.25.25 0 0 1 .192-.41zm-11 2h3.932a.25.25 0 0 0 .192-.41L2.692 6.23a.25.25 0 0 0-.384 0L.342 8.59A.25.25 0 0 0 .534 9z\"\r\n                />\r\n                <path\r\n                  fill-rule=\"evenodd\"\r\n                  d=\"M8 3c-1.552 0-2.94.707-3.857 1.818a.5.5 0 1 1-.771-.636A6.002 6.002 0 0 1 13.917 7H12.9A5.002 5.002 0 0 0 8 3zM3.1 9a5.002 5.002 0 0 0 8.757 2.182.5.5 0 1 1 .771.636A6.002 6.002 0 0 1 2.083 9H3.1z\"\r\n                />\r\n              </svg>\r\n            </button>\r\n            <div class=\"productCard col-8\">\r\n              <select\r\n                class=\"form-select form-select-lg\"\r\n                aria-label=\"ProductType\"\r\n                v-model=\"productCard.chosenType\"\r\n                @change=\"resetOptions\"\r\n              >\r\n                <option disabled value=\"null\">Тип изделия</option>\r\n                <option\r\n                  v-for=\"(product, index) in PRODUCTS\"\r\n                  :key=\"index\"\r\n                  v-html=\"product.ProductName\"\r\n                ></option>\r\n              </select>\r\n            </div>\r\n\r\n            <div class=\"col align-self-end h-100\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-danger w-100 btn-lg px-0\"\r\n                @click=\"$emit('remove')\"\r\n              >\r\n                -\r\n              </button>\r\n            </div>\r\n          </div>\r\n          <low-priority :cardIndex=\"index\"></low-priority>\r\n        </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {products as PRODUCTS, options as OPTIONS} from '../store/product_refs.js';\r\nimport { mapMutations } from \"vuex\";\r\n\r\nimport Edge from \"./options/Edge.vue\";\r\nimport Sink from \"./options/Sink.vue\";\r\nimport Socket from \"./options/Socket.vue\";\r\nimport Cookpanel from \"./options/Cookpanel.vue\";\r\nimport Border from \"./options/Border.vue\";\r\nimport LowPriority from \"./options/LowPriority.vue\";\r\n\r\nexport default {\r\n  name: \"productCard\",\r\n  props: {\r\n    cardIndex: Number\r\n  },\r\n  emits: [\"remove\"],\r\n  data() {\r\n    return {\r\n      PRODUCTS,\r\n      OPTIONS,\r\n      flipped: false,\r\n      selectedAddOn: null,\r\n    };\r\n  },\r\n  created(){\r\n    console.log(this.$store.state.values.product_cards[this.cardIndex])\r\n  },\r\n  computed: {\r\n    productCard() {\r\n      return this.$store.state.values.product_cards[this.cardIndex];\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapMutations([\"updateCardOptions\", \"addCardOption\"]),\r\n    flip() {\r\n      this.flipped = !this.flipped;\r\n    },\r\n  },\r\n  components: {\r\n    Edge,\r\n    Sink,\r\n    Socket,\r\n    Cookpanel,\r\n    Border,\r\n    LowPriority,\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n$rotationspeed: 0.1s;\r\n\r\n@keyframes rotating {\r\n  from {\r\n    transform: rotateY(0deg);\r\n  }\r\n\r\n  to {\r\n    transform: rotateY(90deg);\r\n  }\r\n}\r\n\r\n@keyframes backrotating {\r\n  from {\r\n    transform: rotateY(270deg);\r\n  }\r\n\r\n  to {\r\n    transform: rotateY(360deg);\r\n  }\r\n}\r\n\r\n.flippable {\r\n  transform: scale(1.02);\r\n}\r\n\r\n.back {\r\n  background-color: bisque;\r\n  transform: rotateY(360deg);\r\n}\r\n\r\n.bigcardwrapper {\r\n  transform-style: preserve-3d;\r\n  perspective: 1500px;\r\n}\r\n\r\n.card {\r\n  transition-delay: 0s;\r\n}\r\n\r\n.card-odd {\r\n  background-color: #ffe5b4;\r\n}\r\n\r\n.card-even {\r\n  background-color: #d6ecf5;\r\n}\r\n\r\n.fade-enter-active {\r\n  -webkit-animation: backrotating $rotationspeed forwards;\r\n  -moz-animation: backrotating $rotationspeed forwards;\r\n  -o-animation: backrotating $rotationspeed forwards;\r\n  animation: backrotating $rotationspeed forwards;\r\n}\r\n\r\n.fade-leave-active {\r\n  -webkit-animation: rotating $rotationspeed forwards;\r\n  -moz-animation: rotating $rotationspeed forwards;\r\n  -o-animation: rotating $rotationspeed forwards;\r\n  animation: rotating $rotationspeed forwards;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}