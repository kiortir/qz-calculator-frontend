resetOptions\"\r\n              >\r\n                <option disabled value=\"null\">Тип изделия</option>\r\n                <option\r\n                  v-for=\"(product, index) in products\"\r\n                  :key=\"index\"\r\n                  v-html=\"product.ProductName\"\r\n                ></option>\r\n              </select>\r\n            </div>\r\n            <div class=\"col align-self-end h-100\">\r\n              <button\r\n                type=\"button\"\r\n                class=\"btn btn-danger w-100 btn-lg px-0\"\r\n                @click=\"$emit('remove')\"\r\n              >\r\n                -\r\n              </button>\r\n            </div>\r\n          </div>\r\n          <div class=\"row-cols-md-2 row-cols-1 row gx-3 gy-2\">\r\n            <div\r\n              class=\"col\"\r\n              v-for=\"(option, option_index) in productCard.option_card\"\r\n              :key=\"option_index + index\"\r\n            >\r\n              <component\r\n                class=\"h-100\"\r\n                :is=\"option.component_type\"\r\n                :index=\"option_index\"\r\n                :key=\"option_index + index.toString()\"\r\n                :cardId=\"index\"\r\n                :compData=\"option\"\r\n                @removeoption=\"removeOption(option_index)\"\r\n              ></component>\r\n            </div>\r\n          </div>\r\n          <div v-if=\"productCard.chosenType != null\" class=\"row mt-4 g-1\">\r\n            <div class=\"col-10\">\r\n              <select\r\n                class=\"form-select\"\r\n                aria-label=\".form-select-sm example\"\r\n                v-model=\"selectedAddOn\"\r\n              >\r\n                <option value=\"null\" disabled>Выберите опцию</option>\r\n                <option\r\n                  v-for=\"(option, opt_index) in options\"\r\n                  :key=\"opt_index\"\r\n                  :value=\"option.OptionId\"\r\n                  v-html=\"option.OptionName\"\r\n     