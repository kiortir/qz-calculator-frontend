laceState(\n            JSON.parse(document.getElementById(\"values\").textContent)\n        );\n        this.tabs.push(\n            ...[{\n                    component: \"materials\",\n                    name: \"Материал\",\n                },\n                {\n                    component: \"products\",\n                    name: \"Изделия\",\n                },\n                {\n                    component: \"logistics\",\n                    name: \"Логистика\",\n                },\n            ]\n        );\n    },\n};\n</script>\n\n<style>\n#onload-block {\n    display: none;\n}\n</style>\n"]}]}