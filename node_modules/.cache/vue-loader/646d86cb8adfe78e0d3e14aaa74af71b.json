   @input=\"updateStorage('materialName', $event.target.value)\"\r\n              type=\"text\"\r\n              class=\"form-control border-info\"\r\n              placeholder=\"название материала\"\r\n            />\r\n            <label :for=\"'materialname-' + index\">название материала</label>\r\n          </div>\r\n          <div class=\"col-12 col-sm-8 form-floating\">\r\n            <input\r\n              :id=\"'materialprice-' + index\"\r\n              :type=\"isFocused ? 'number' : 'text'\"\r\n              :value=\"formattedSlabPrice\"\r\n              @input=\"cardData.materialPrice = $event.target.value\"\r\n              class=\"form-control\"\r\n              placeholder=\"цена слэба\"\r\n              @blur=\"focusOut\"\r\n              @focus=\"focusIn\"\r\n            />\r\n            <label :for=\"'materialprice-' + index\">цена слэба</label>\r\n          </div>\r\n          <div class=\"col-12 col-sm-4 form-floating\">\r\n            <input\r\n              :id=\"'slabcount-' + index\"\r\n              :type=\"countIsFocused ? 'number' : 'text'\"\r\n              :value=\"formattedSlabCount\"\r\n              @input=\"cardData.materialCount = $event.target.value\"\r\n              class=\"form-control\"\r\n              placeholder=\"число слэбов\"\r\n              @blur=\"focusOutCount\"\r\n              @focus=\"focusInCount\"\r\n            />\r\n            <label :for=\"'slabcount-' + index\">число слэбов</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-12 col-sm-1\">\r\n        <button\r\n          type=\"button\"\r\n          class=\"btn btn-danger h-100 w-100 py-0 px-md-0\"\r\n          @click=\"deleteCard\"\r\n        >\r\n          -\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: \"MaterialCard\",\r\n  props: [\"index\"],\r\n  emits: [\"remove\"],\r\n  data() {\r\n    return {\r\n      capitalize(str) {\r\n        return str\r\n          .split(\" \")\r\n          .map((word) => word.charAt(0).toUpperCase() + word.slice(1))\r\n          .join(\"\");\r\n      },\r\n      isFocused: false,\r\n      countIsFocused: false,\r\n      formattedSlabPrice: this.$store.state.material_cards[this.index]\r\n        .materialPrice\r\n        ? this.$store.state.material_cards[this.index].materialPrice\r\n            .toString()\r\n            .replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1 \") + \" руб/шт.\"\r\n        : null,\r\n      formattedSlabCount: this.$store.state.material_cards[this.index]\r\n        .materialCount\r\n        ? this.$store.state.material_cards[this.index].materialCount + \" шт.\"\r\n        : null,\r\n    };\r\n  },\r\n  computed: {\r\n    cardData() {\r\n      return this.$store.state.material_cards[this.index];\r\n    },\r\n  },\r\n  methods: {\r\n    updateStorage(field, value){\r\n      this.$store.commit('updateMaterial', {index: this.index, field, value})\r\n    },\r\n\r\n\r\n    deleteCard() {\r\n      this.$store.commit(\"deleteMaterialCard\", this.index);\r\n    },\r\n    focusOut: function () {\r\n      this.isFocused = false;\r\n      this.formattedSlabPrice =\r\n        this.cardData.materialPrice\r\n          .toString()\r\n          .replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1 \") + \" руб/шт.\";\r\n      console.log(this.formattedSlabPrice);\r\n      if (this.formattedSlabPrice == \" руб/шт.\") {\r\n        this.formattedSlabPrice = null;\r\n      }\r\n    },\r\n    focusIn: function () {\r\n      this.isFocused = true;\r\n      this.formattedSlabPrice = this.cardData.materialPrice;\r\n    },\r\n    focusOutCount: function () {\r\n      this.countIsFocused = false;\r\n\r\n      this.formattedSlabCount =\r\n        this.cardData.materialCount\r\n          .toString()\r\n          .replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1 \") + \" шт.\";\r\n    },\r\n    focusInCount: function () {\r\n      this.countIsFocused = true;\r\n      this.formattedSlabCount = this.cardData.materialCount;\r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}