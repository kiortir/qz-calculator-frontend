rialCard.vue\";\n\nexport default {\n  name: \"Material\",\n  emits: [\"focusCard\"],\n  computed: {\n    cards() {\n      return this.$store.state.values.material_cards;\n    },\n  },\n  methods: {\n    ...mapMutations([\"addMaterialCard\", \"removeMaterialCard\"]),\n    // newCard() {\n    //   this.addMaterialCard();\n    //   let id = this.cards.length;\n    //   this.$emit(\"focusCard\", \"material-card\", id - 1);\n    // },\n    getRandomInt(min, max) {\n      min = Math.ceil(min);\n      max = Math.floor(max);\n      return Math.floor(Math.random() * (max - min)) + min; //Максимум не включается, минимум включается\n    },\n  },\n  components: {\n    MaterialCard,\n  },\n};\n</script>\n\n<style>\n</style>"]}]}