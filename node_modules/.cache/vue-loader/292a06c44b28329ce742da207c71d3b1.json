class=\"list-group-item\">\r\n      <h4 class=\"mb-1\">Материал</h4>\r\n      <ul>\r\n        <li\r\n          v-for=\"(material, index) of materials\"\r\n          :key=\"index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col h6 my-auto\" v-if=\"material.name\">{{\r\n            material.name\r\n          }}</span>\r\n          <span class=\"col my-auto\" v-else>Кварцевый агломерат</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{ material.price }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n        <li\r\n          class=\"row justify-content-between g-0 mt-2\"\r\n          v-if=\"materialSum != 0\"\r\n        >\r\n          <span class=\"col my-auto fw-light\">Подытог:</span>\r\n          <span class=\"col text-end my-auto fw-bold\"\r\n            >{{ materialSum }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"list-group-item\">\r\n      <h4 class=\"mb-1\">Изделия</h4>\r\n      <ul>\r\n        <li\r\n          v-for=\"(product, product_index) of products\"\r\n          :key=\"product_index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col my-auto\">{{ product.name }}</span>\r\n          <span\r\n            class=\"col text-end my-auto fw-bold\"\r\n            v-html=\"productSum(product_index) + '&nbsp;руб.'\"\r\n          ></span>\r\n          <ul class=\"col-12\">\r\n            <li\r\n              class=\"small-text row justify-content-between g-0 mb-2\"\r\n              v-for=\"(option, option_index) of product.options\"\r\n              :key=\"option_index\"\r\n            >\r\n              <span class=\"col my-auto\">{{ option.text }}</span>\r\n              <span class=\"col text-end my-auto\"\r\n                >{{ option.result }}&nbsp;руб.</span\r\n              >\r\n            </li>\r\n          </ul>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import * as formula from '../store/formulas';\r\n\r\nexport default {\r\n  computed: {\r\n    materials() {\r\n      return this.$store.getters.materials;\r\n    },\r\n    materialSum() {\r\n      return this.materials.length > 1\r\n        ? this.materials.reduce((x, y) => ({ price: x.price + y.price })).price\r\n        : 0;\r\n    },\r\n    products() {\r\n      return this.$store.getters.products;\r\n    },\r\n    productSum(card_index) {\r\n      console.log(this.products);\r\n      console.log(card_index);\r\n      return this.products[card_index].options.length > 1\r\n        ? this.products[card_index].options.reduce((x, y) => ({\r\n            result: x.result + y.result,\r\n          })).result\r\n        : 0;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.small-text {\r\n  font-size: 0.8em;\r\n}\r\nul {\r\n  padding-left: 1rem !important;\r\n}\r\n</style>"]}]}