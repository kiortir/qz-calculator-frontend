<template>\r\n\r\n<div></div>\r\n</template>\r\n\r\n<script>\r\nimport Edge from \"./options/Edge.vue\";\r\nimport Sink from \"./options/Sink.vue\";\r\nimport Socket from \"./options/Socket.vue\";\r\nimport Cookpanel from \"./options/Cookpanel.vue\";\r\nimport Border from \"./options/Border.vue\";\r\nimport LowPriority from \"./options/LowPriority.vue\";\r\n\r\nconst products_list = JSON.parse(\r\n  document.getElementById(\"predefined-products-data\").textContent\r\n);\r\nlet options_list = JSON.parse(document.getElementById(\"options\").textContent);\r\n\r\nlet edge_data = {\r\n  data: {\r\n    width: null,\r\n    length: null,\r\n    type: null,\r\n    angle: null,\r\n  },\r\n};\r\n\r\nlet sink_data = {\r\n  data: {\r\n    quantity: null,\r\n    type: null,\r\n  },\r\n};\r\n\r\nlet border_data = {\r\n  data: {\r\n    type: \"Простой\",\r\n    length: null,\r\n  },\r\n};\r\nlet socket_data = {\r\n  data: {\r\n    quantity: null,\r\n  },\r\n};\r\nlet cookpanel_data = {\r\n  data: {\r\n    quantity: null,\r\n    type: null,\r\n  },\r\n};\r\n\r\nconst option_ref = {\r\n  edge: edge_data,\r\n  sink: sink_data,\r\n  border: border_data,\r\n  socket: socket_data,\r\n  cookpanel: cookpanel_data,\r\n};\r\n\r\nexport default {\r\n  name: \"productCard\",\r\n  props: [\"index\", \"data\"],\r\n  emits: [\"remove\"],\r\n  data() {\r\n    return {\r\n      flipped: false,\r\n      products: products_list,\r\n      selectedAddOn: null,\r\n      options: options_list,\r\n    };\r\n  },\r\n  computed: {\r\n    productCard() {\r\n      return this.$store.state.product_cards[this.index];\r\n    },\r\n    isHolded() {\r\n      return this.holdStatus;\r\n    },\r\n  },\r\n  methods: {\r\n    addOption() {\r\n      if (this.selectedAddOn != null) {\r\n        this.productCard.option_card.push({\r\n          component_type: this.selectedAddOn,\r\n          data: JSON.parse(JSON.stringify(option_ref[this.selectedAddOn].data)),\r\n        });\r\n      }\r\n    },\r\n    resetOptions() {\r\n      this.$store.commit(\"resetOptions\", this.index);\r\n    },\r\n    removeOption(index) {\r\n      this.productCard.option_card.splice(index, 1);\r\n    },\r\n    flip() {\r\n      this.flipped = !this.flipped;\r\n    },\r\n  },\r\n  components: {\r\n    Edge,\r\n    Sink,\r\n    Socket,\r\n    Cookpanel,\r\n    Border,\r\n    LowPriority,\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n$rotationspeed: 0.1s;\r\n\r\n@keyframes rotating {\r\n  from {\r\n    transform: rotateY(0deg);\r\n  }\r\n\r\n  to {\r\n    transform: rotateY(90deg);\r\n  }\r\n}\r\n\r\n@keyframes backrotating {\r\n  from {\r\n    transform: rotateY(270deg);\r\n  }\r\n\r\n  to {\r\n    transform: rotateY(360deg);\r\n  }\r\n}\r\n\r\n.flippable {\r\n  transform: scale(1.02);\r\n}\r\n\r\n.back {\r\n  background-color: bisque;\r\n  transform: rotateY(360deg);\r\n}\r\n\r\n.bigcardwrapper {\r\n  transform-style: preserve-3d;\r\n  perspective: 1500px;\r\n}\r\n\r\n.card {\r\n  transition-delay: 0s;\r\n}\r\n\r\n.card-odd {\r\n  background-color: #ffe5b4;\r\n}\r\n\r\n.card-even {\r\n  background-color: #d6ecf5;\r\n}\r\n\r\n.fade-enter-active {\r\n  -webkit-animation: backrotating $rotationspeed forwards;\r\n  -moz-animation: backrotating $rotationspeed forwards;\r\n  -o-animation: backrotating $rotationspeed forwards;\r\n  animation: backrotating $rotationspeed forwards;\r\n}\r\n\r\n.fade-leave-active {\r\n  -webkit-animation: rotating $rotationspeed forwards;\r\n  -moz-animation: rotating $rotationspeed forwards;\r\n  -o-animation: rotating $rotationspeed forwards;\r\n  animation: rotating $rotationspeed forwards;\r\n}\r\n</style>\r\n"]}]}