col-lg-7 gx-2 mb-5\">\n      <div class=\"infocard\">\n        <ul class=\"nav nav-tabs nav-fill nav-dark sticky-top mb-4 rounded\">\n          <li\n            class=\"nav-item bg-light\"\n            v-for=\"(tab, index) of tabs\"\n            :key=\"tab.component\"\n          >\n            <button\n              class=\"nav-link\"\n              data-bs-toggle=\"tab\"\n              type=\"button\"\n              :id=\"`head-${tab.component}`\"\n              :data-bs-target=\"`#${tab.component}`\"\n              :class=\"{ active: index == 0 }\"\n              v-html=\"tab.name\"\n            ></button>\n          </li>\n        </ul>\n\n        <div class=\"tab-content\" id=\"mainContent\">\n          <div\n            v-for=\"(tab, index) of tabs\"\n            class=\"tab-pane fade px-2 p-lg-0\"\n            :id=\"tab.component\"\n            :key=\"tab.component\"\n            :class=\"{ active: index == 0, show: index == 0 }\"\n          >\n            <component\n              :is=\"tab.component\"\n              :key=\"tab.component\"\n              @focusCard=\"focusCard\"\n            ></component>\n          </div>\n        </div>\n        <div\n          class=\"offcanvas offcanvas-bottom h-100 border-0\"\n          tabindex=\"-1\"\n          id=\"offcanvas-result\"\n        >\n          <div class=\"offcanvas-header\">\n            <h5 class=\"offcanvas-title\">Итог</h5>\n            <button type=\"button\" class=\"btn btn-primary\">Создать КП</button>\n          </div>\n          <div class=\"offcanvas-body container\">\n            <div class=\"infocard\">\n              <!-- <result></result> -->\n            </div>\n            <div class=\"container-fluid mb-2 fixed-bottom g-2 mx-0\">\n              <button\n                type=\"button\"\n                class=\"btn btn-danger w-100\"\n                data-bs-dismiss=\"offcanvas\"\n                aria-label=\"Close\"\n                v-on:touchstart.passive=\"saveCalc\"\n              >\n                Закрыть\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"fixed-bottom mb-2 container d-block d-lg-none\">\n      <button\n        class=\"btn btn-primary w-100\"\n        type=\"button\"\n        data-bs-toggle=\"offcanvas\"\n        data-bs-target=\"#offcanvas-result\"\n        v-on:touchstart.passive=\"saveCalc\"\n      >\n        Итог\n      </button>\n    </div>\n\n    <div class=\"container col-5 d-none d-lg-block\">\n      <div class=\"card sticky-top mb-2\">\n        <div class=\"card-header bg-card-even row gx-0\">\n          <ul class=\"nav nav-tabs card-header-tabs col\">\n            <li class=\"nav-item\">\n              <button\n                class=\"nav-link active\"\n                id=\"home-tab\"\n                data-bs-toggle=\"tab\"\n                data-bs-target=\"#home\"\n                type=\"button\"\n                role=\"tab\"\n                aria-controls=\"home\"\n                aria-selected=\"true\"\n              >\n                Итог\n              </button>\n            </li>\n            <li class=\"nav-item\">\n              <button\n                class=\"nav-link\"\n                id=\"salary-tab\"\n                data-bs-toggle=\"tab\"\n                data-bs-target=\"#salary\"\n                type=\"button\"\n                role=\"tab\"\n                aria-controls=\"salary\"\n                aria-selected=\"false\"\n              >\n                Зарплата\n              </button>\n            </li>\n          </ul>\n          <div class=\"col-3\">\n            <button class=\"btn btn-primary p-0 w-100\" @click=\"saveCalc\">\n              Сохранить\n            </button>\n          </div>\n        </div>\n        <div class=\"card-body result-block\">\n          <div class=\"tab-content pb-4\">\n            <div\n              class=\"tab-pane fade show active overflow-visible\"\n              id=\"home\"\n              role=\"tabpanel\"\n              aria-labelledby=\"home-tab\"\n            >\n              <!-- <result></result> -->\n            </div>\n            <div class=\"tab-pane fade\" id=\"salary\" role=\"tabpanel\">\n              <!-- <salary></salary> -->\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport store from \"./store\";\n\nimport Materials from \"./components/Materials.vue\";\nimport Products from \"./components/Products.vue\";\nimport Logistics from \"./components/Logistics.vue\";\n\nvar lastUserInput = null;\n\nstore.subscribe(() => {\n  let now = new Date();\n  console.log(now.value());\n  lastUserInput = now.value();\n});\n\nfunction getCookie(name) {\n  let cookieValue = null;\n  if (document.cookie && document.cookie !== \"\") {\n    const cookies = document.cookie.split(\";\");\n    for (let i = 0; i < cookies.length; i++) {\n      const cookie = cookies[i].trim();\n      // Does this cookie string begin with the name we want?\n      if (cookie.substring(0, name.length + 1) === name + \"=\") {\n        cookieValue = decodeURIComponent(cookie.substring(name.length + 1));\n        break;\n      }\n    }\n  }\n  return cookieValue;\n}\nconst csrftoken = getCookie(\"csrftoken\");\n\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      tabs: [\n        {\n          component: \"materials\",\n          name: \"Материал\",\n        },\n        {\n          component: \"products\",\n          name: \"Изделия\",\n        },\n        {\n          component: \"logistics\",\n          name: \"Логистика\",\n        },\n      ],\n    };\n  },\n  computed: {\n    lastInput() {\n      return lastUserInput;\n    },\n  },\n  methods: {\n    focusCard(tab, id) {\n      setTimeout(function () {\n        let element = document.getElementById(`${tab}-${id}`);\n        element.scrollIntoView({\n          block: \"center\",\n          behavior: \"auto\",\n        });\n      }, 0);\n    },\n    saveCalc() {\n      this.axios\n        .put(\"save\", csrftoken, {\n          data: { state: this.$store.state, updateTime: this.lastInput },\n        })\n        .then(function (response) {\n          console.log(this.lastInput);\n          alert(response.data);\n        })\n        .catch(function (error) {\n          alert(error);\n        });\n    },\n  },\n  components: {\n    Materials,\n    Products,\n    Logistics,\n  },\n};\n</script>\n\n<style>\n#mainContent {\n  padding-bottom: 50vh !important;\n}\n\n#onload-block {\n  display: none;\n}\n</style>\n"]}]}