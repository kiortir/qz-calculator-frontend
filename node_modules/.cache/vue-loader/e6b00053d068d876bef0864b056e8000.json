lculator/main/frontend/src/App.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"position-absolute w-100 h-100\">\n    <div class=\"row mx-0 w-100 px-0\">\n      <div class=\"col-lg-7 gx-2 mb-5\">\n        <div class=\"infocard\">\n          <ul class=\"nav nav-tabs nav-fill nav-dark sticky-top mb-4 rounded\">\n            <li\n              class=\"nav-item bg-light\"\n              v-for=\"(tab, index) of tabs\"\n              :key=\"tab.component\"\n            >\n              <button\n                class=\"nav-link\"\n                data-bs-toggle=\"tab\"\n                type=\"button\"\n                :id=\"`head-${tab.component}`\"\n                :data-bs-target=\"`#${tab.component}`\"\n                :class=\"{ active: index == 0 }\"\n                @click=\"chosenTab = tab.component\"\n                v-html=\"tab.name\"\n              ></button>\n            </li>\n          </ul>\n\n          <div class=\"tab-content\" id=\"mainContent\">\n            <div\n              v-for=\"(tab, index) of tabs\"\n              class=\"tab-pane fade px-2 p-lg-0\"\n              :id=\"tab.component\"\n              :key=\"tab.component\"\n              :class=\"{ active: index == 0, show: index == 0 }\"\n            >\n              <component\n                :is=\"tab.component\"\n                :key=\"tab.component\"\n                @focusCard=\"focusCard\"\n              ></component>\n            </div>\n          </div>\n          <div\n            class=\"offcanvas offcanvas-bottom h-100 border-0\"\n            tabindex=\"-1\"\n            id=\"offcanvas-result\"\n          >\n            <div class=\"offcanvas-header\">\n              <h5 class=\"offcanvas-title\">Итог</h5>\n              <button type=\"button\" class=\"btn btn-primary\">Создать КП</button>\n            </div>\n            <div class=\"offcanvas-body container\">\n              <div class=\"infocard\">\n                <result></result>\n              </div>\n              <div class=\"container mb-2 fixed-bottom\">\n                <button\n                  type=\"button\"\n                  class=\"btn btn-danger w-100\"\n                  data-bs-dismiss=\"offcanvas\"\n                  aria-label=\"Close\"\n                >\n                  Закрыть\n                </button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"col-5 d-none d-lg-block\">\n        <div class=\"card sticky-top mb-2\">\n          <div class=\"card-header bg-card-even row gx-0\">\n            <ul class=\"nav nav-tabs card-header-tabs col\">\n              <li class=\"nav-item\">\n                <button\n                  class=\"nav-link active\"\n                  id=\"home-tab\"\n                  data-bs-toggle=\"tab\"\n                  data-bs-target=\"#home\"\n                  type=\"button\"\n                  role=\"tab\"\n                  aria-controls=\"home\"\n                  aria-selected=\"true\"\n                >\n                  Итог\n                </button>\n              </li>\n              <li class=\"nav-item\">\n                <button\n                  class=\"nav-link\"\n                  id=\"salary-tab\"\n                  data-bs-toggle=\"tab\"\n                  data-bs-target=\"#salary\"\n                  type=\"button\"\n                  role=\"tab\"\n                  aria-controls=\"salary\"\n                  aria-selected=\"false\"\n                >\n                  Зарплата\n                </button>\n              </li>\n            </ul>\n            <div class=\"col-3\">\n              <button class=\"btn btn-primary p-0 w-100\" @click=\"save\">\n                Сохранить\n              </button>\n            </div>\n          </div>\n          <div class=\"card-body px-0\">\n            <div class=\"tab-content pb-4 overflow-auto result-block\">\n              <div\n                class=\"tab-pane fade show active\"\n                id=\"home\"\n                role=\"tabpanel\"\n                aria-labelledby=\"home-tab\"\n              >\n                <result></result>\n              </div>\n              <div class=\"tab-pane fade\" id=\"salary\" role=\"tabpanel\">\n                <!-- <salary></salary> -->\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"fixed-bottom mb-2 container d-block d-lg-none\">\n      <div class=\"row\">\n        <div class=\"col\">\n          <button\n            class=\"btn btn-primary w-100\"\n            type=\"button\"\n            data-bs-toggle=\"offcanvas\"\n            data-bs-target=\"#offcanvas-result\"\n            v-on:touchstart.passive=\"save\"\n          >\n            Итог\n          </button>\n        </div>\n        <div class=\"col\" v-if=\"chosenTab != 'logistics'\">\n          <button class=\"btn btn-primary w-100\" type=\"button\" @click=\"addCard\">\n            +\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapMutations } from \"vuex\";\n\nimport Materials from \"./components/Materials.vue\";\nimport Products from \"./components/Products.vue\";\nimport Logistics from \"./components/Logistics.vue\";\n\nimport Result from \"./components/Result.vue\";\n\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      tabs: [\n        {\n          component: \"materials\",\n          name: \"Материал\",\n        },\n        {\n          component: \"products\",\n          name: \"Изделия\",\n        },\n        {\n          component: \"logistics\",\n          name: \"Логистика\",\n        },\n      ],\n      chosenTab: \"materials\",\n    };\n  },\n  methods: {\n    ...mapMutations([\"addMaterialCard\", \"addProductCard\"]),\n    ...mapActions([\"save\"]),\n    focusCard(tab, id) {\n      setTimeout(function () {\n        let element = document.getElementById(`${tab}-${id}`);\n        element.scrollIntoView({\n          block: \"center\",\n          behavior: \"auto\",\n        });\n      }, 0);\n    },\n    addCard() {\n      let functions = {\n        materials: this.addMaterialCard,\n        products: this.addProductCard,\n      };\n      functions[this.chosenTab]();\n    },\n  },\n  components: {\n    Materials,\n    Products,\n    Logistics,\n    Result,\n  },\n};\n</script>\n\n<style>\n#mainContent {\n  padding-bottom: 50vh !important;\n}\n\n#onload-block {\n  display: none;\n}\n\n.small-text {\n  font-size: 0.8em;\n}\n\n.recommended-discount {\n  border-color: green !important;\n  color: green !important;\n}\n.recommended-discount:hover {\n  background-color: green !important;\n  color: white !important;\n}\n.btn-check:checked + .btn-outline-primary.recommended-discount {\n  background-color: green !important;\n  color: white !important;\n}\n\n.result-block {\n  height: 98vh;\n  position: relative;\n}\n.main-container {\n  overflow-x: hidden !important;\n  position: relative;\n}\n\n#app {\n  height: 100%;\n  width: 100%;\n}\n\n.foot {\n  display: table-row;\n  vertical-align: bottom;\n  height: 1px;\n}\n</style>\n"]}]}