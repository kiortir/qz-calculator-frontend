class=\"list-group-item\">\r\n      <h4 class=\"mb-1\">Материал</h4>\r\n      <ul>\r\n        <li\r\n          v-for=\"(material, index) of materials\"\r\n          :key=\"index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col h6 my-auto\" v-if=\"material.name\">{{\r\n            material.name\r\n          }}</span>\r\n          <span class=\"col my-auto\" v-else>Кварцевый агломерат</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{\r\n              material.price + overPrice.result / materials.length\r\n            }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n\r\n        <li class=\"row justify-content-between g-0 mt-2\">\r\n          <span class=\"col h6 my-auto\">Материалы / расходники</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{ measurementPrice.result }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n\r\n        <li\r\n          class=\"row justify-content-between g-0 mt-2\"\r\n          v-if=\"materialSum != 0\"\r\n        >\r\n          <span class=\"col my-auto fw-light\">Подытог:</span>\r\n          <span class=\"col text-end my-auto fw-bold\"\r\n            >{{\r\n              materialSum.price + measurementPrice.result + overPrice.result\r\n            }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"list-group-item\" v-if=\"products.length\">\r\n      <h4 class=\"mb-1\">Изделия</h4>\r\n      <ul>\r\n        <li\r\n          v-for=\"(product, product_index) of products\"\r\n          :key=\"product_index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col my-auto\">{{ product.name }}</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{\r\n              optionSum[product_index].result + addonSum[product_index].result\r\n            }}&nbsp;руб.</span\r\n          >\r\n          <ul class=\"col-12\">\r\n            <li\r\n              class=\"small-text row justify-content-between g-0 mb-2\"\r\n              v-for=\"(option, option_index) of product.options\"\r\n              :key=\"option_index\"\r\n            >\r\n              <span class=\"col my-auto\">{{ option.text }}</span>\r\n              <span class=\"col text-end my-auto\"\r\n                >{{ option.result }}&nbsp;руб.</span\r\n              >\r\n            </li>\r\n            <li\r\n              class=\"small-text row justify-content-between g-0 mb-2\"\r\n              v-for=\"(addon, addon_index) of addons[product_index]\"\r\n              :key=\"addon_index\"\r\n            >\r\n              <span class=\"col my-auto\">{{ addon.name }}</span>\r\n              <span class=\"col text-end my-auto\"\r\n                >{{ addon.result }}&nbsp;руб.</span\r\n              >\r\n            </li>\r\n          </ul>\r\n        </li>\r\n        <li\r\n          class=\"row justify-content-between g-0 mt-2\"\r\n          v-if=\"optionSum.length > 1\"\r\n        >\r\n          <span class=\"col my-auto fw-light\">Подытог:</span>\r\n          <span class=\"col text-end my-auto fw-bold\"\r\n            >{{ productSum.result }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"list-group-item\" v-if=\"logisticsList.length\">\r\n      <h4>Логистика</h4>\r\n      <ul>\r\n        <li\r\n          v-for=\"(logistic, index) of logisticsList\"\r\n          :key=\"index\"\r\n          class=\"row justify-content-between g-0 mb-2\"\r\n        >\r\n          <span class=\"col h6 my-auto\">{{ logistic.text }}</span>\r\n          <span class=\"col text-end my-auto\"\r\n            >{{ logistic.result }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n\r\n        <li\r\n          class=\"row justify-content-between g-0 mt-2\"\r\n          v-if=\"logisticSum.result != 0\"\r\n        >\r\n          <span class=\"col my-auto fw-light\">Подытог:</span>\r\n          <span class=\"col text-end my-auto fw-bold\"\r\n            >{{ logisticSum.result }}&nbsp;руб.</span\r\n          >\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div class=\"list-group-item\">\r\n      <h4>Итог: {{ total }}</h4>\r\n      <div class=\"btn-group w-100\" role=\"group\">\r\n        <input\r\n          type=\"radio\"\r\n          class=\"btn-check\"\r\n          name=\"discountRadio\"\r\n          id=\"discount0\"\r\n          value=\"0\"\r\n          autocomplete=\"off\"\r\n          v-model=\"discount\"\r\n        />\r\n        <label class=\"btn btn-outline-primary\" for=\"discount0\"\r\n          >Без скидки</label\r\n        >\r\n\r\n        <input\r\n          type=\"radio\"\r\n          class=\"btn-check\"\r\n          name=\"discountRadio\"\r\n          id=\"discount10\"\r\n          value=\"10\"\r\n          autocomplete=\"off\"\r\n          v-model=\"discount\"\r\n        />\r\n        <label class=\"btn btn-outline-primary\" for=\"discount10\"\r\n          >Колонка 1</label\r\n        >\r\n\r\n        <input\r\n          type=\"radio\"\r\n          class=\"btn-check\"\r\n          name=\"discountRadio\"\r\n          id=\"discount20\"\r\n          value=\"20\"\r\n          autocomplete=\"off\"\r\n          v-model=\"discount\"\r\n        />\r\n        <label class=\"btn btn-outline-primary\" for=\"discount20\"\r\n          >Колонка 2</label\r\n        >\r\n\r\n        <input\r\n          type=\"radio\"\r\n          class=\"btn-check\"\r\n          name=\"discountRadio\"\r\n          id=\"discount30\"\r\n          value=\"30\"\r\n          autocomplete=\"off\"\r\n          v-model=\"discount\"\r\n        />\r\n        <label class=\"btn btn-outline-primary\" for=\"discount30\"\r\n          >Колонка 3</label\r\n        >\r\n      </div>\r\n    </div>\r\n    Траты: {{ spendings }} Маржа: {{ recommendedDiscount }}\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  computed: {\r\n    materials() {\r\n      return this.$store.getters.materials;\r\n    },\r\n    materialSum() {\r\n      return this.materials.length\r\n        ? this.materials.reduce((x, y) => ({\r\n            price: x.price + y.price,\r\n            spendings: x.spendings + y.spendings,\r\n          }))\r\n        : 0;\r\n    },\r\n    products() {\r\n      return this.$store.getters.products;\r\n    },\r\n    optionSum() {\r\n      return this.products.map((product, index) =>\r\n        product.options.length > 0\r\n          ? this.products[index].options.reduce((x, y) => ({\r\n              result: x.result + y.result,\r\n              salary: x.salary + y.salary,\r\n              consumables: x.consumables + y.consumables,\r\n            }))\r\n          : { result: 0, salary: 0 }\r\n      );\r\n    },\r\n    productSum() {\r\n      return this.optionSum.length > 0\r\n        ? this.optionSum.reduce((x, y) => ({\r\n            result: x.result + y.result,\r\n            salary: x.salary + y.salary,\r\n            consumables: x.consumables + y.consumables,\r\n          }))\r\n        : {\r\n            result: 0,\r\n            salary: 0,\r\n            consumables: 0,\r\n          };\r\n    },\r\n    installation() {\r\n      return this.$store.getters.installationPrice;\r\n    },\r\n    delivery() {\r\n      return this.$store.getters.deliveryPrice;\r\n    },\r\n    additionalWork() {\r\n      return {\r\n        result: this.$store.state.values.logistics.additionalWork,\r\n        text: \"Дополнительные работы\",\r\n      };\r\n    },\r\n    lifting() {\r\n      return this.$store.getters.manualLifting;\r\n    },\r\n    templates() {\r\n      return {\r\n        result: this.$store.state.values.logistics.templatePrice,\r\n        text: \"Изготовление шаблонов\",\r\n      };\r\n    },\r\n    logisticsList() {\r\n      return [\r\n        this.lifting,\r\n        this.additionalWork,\r\n        this.delivery,\r\n        this.installation,\r\n        this.templates,\r\n      ].filter((price) => price.result);\r\n    },\r\n    logisticSum() {\r\n      return this.logisticsList.length > 0\r\n        ? this.logisticsList.reduce((x, y) => ({\r\n            result: x.result + y.result,\r\n            salary: x.salary + y.salary,\r\n          }))\r\n        : {\r\n            result: 0,\r\n            salary: 0,\r\n          };\r\n    },\r\n    measurementPrice() {\r\n      return this.$store.getters.measurementPrice;\r\n    },\r\n    overPrice() {\r\n      return {\r\n        result: Number(this.$store.state.values.logistics.overPrice),\r\n      };\r\n    },\r\n    addons() {\r\n      return this.$store.getters.addons;\r\n    },\r\n    addonSum() {\r\n      return this.addons.map((addon) => {\r\n        if (addon.length > 0) {\r\n          let res = addon.reduce((x, y) => {\r\n            return {\r\n              result: x.result + y.result,\r\n              salary: x.salary + y.salary,\r\n            };\r\n          });\r\n          res.result = Math.ceil(res.result * (1 - this.discount / 100));\r\n          return res;\r\n        } else {\r\n          return {\r\n            result: 0,\r\n            salary: 0,\r\n          };\r\n        }\r\n      });\r\n    },\r\n    total() {\r\n      return [\r\n        this.materialSum.price,\r\n        this.measurementPrice.result,\r\n        this.overPrice.result,\r\n        this.productSum.result,\r\n        this.logisticSum.result,\r\n      ].reduce((x, y) => x + y);\r\n    },\r\n    discount: {\r\n      get() {\r\n        return typeof this.$store.state.values.discount == \"undefined\"\r\n          ? 0\r\n          : this.$store.state.values.discount;\r\n      },\r\n      set(value) {\r\n        this.setDiscount(value);\r\n      },\r\n    },\r\n    spendings() {\r\n      let material_spendings = this.materialSum.spendings;\r\n      let product_spendings =\r\n        this.productSum.salary +\r\n        this.productSum.consumables +\r\n        (this.addonSum.length\r\n          ? this.addonSum.reduce((x, y) => ({ salary: x.salary + y.salary }))\r\n              .salary\r\n          : 0);\r\n      let logistic_spendings =\r\n        this.logisticSum.salary + this.measurementPrice.salary;\r\n      return material_spendings + product_spendings + logistic_spendings;\r\n    },\r\n    recommendedDiscount() {\r\n      return this.spendings / this.total;\r\n    },\r\n  },\r\n  methods: {\r\n    setDiscount(value) {\r\n      this.$store.commit(\"setDiscount\", value);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.small-text {\r\n  font-size: 0.8em;\r\n}\r\nul {\r\n  padding-left: 1rem !important;\r\n}\r\n</style>"]}]}