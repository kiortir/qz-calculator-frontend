            <label :for=\"'materialprice-' + card_index\">цена слэба</label>\r\n          </div>\r\n          <div class=\"col-12 col-sm-4 form-floating\">\r\n            <input\r\n              :id=\"'material-count-' + card_index\"\r\n              :value=\"cardData.materialCount\"\r\n              @input=\"updateMaterialCount({index: card_index, value: $event.target.value})\"\r\n              type=\"number\"\r\n              class=\"form-control\"\r\n              placeholder=\"число слэбов\"\r\n            />\r\n            <label :for=\"'material-count-' + card_index\">число слэбов</label>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-12 col-sm-1\">\r\n        <button\r\n          type=\"button\"\r\n          class=\"btn btn-danger h-100 w-100 py-0 px-md-0\"\r\n          @click=\"$emit('remove')\"\r\n        >\r\n          -\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapMutations } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"MaterialCard\",\r\n  props: {\r\n    card_index: Number,\r\n  },\r\n  emits: [\"remove\"],\r\n  computed: {\r\n    cardData() {\r\n      return this.$store.state.values.material_cards[this.card_index];\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapMutations([\"updateMaterialName\", \"updateMaterialCount\", 'updateMaterialPrice']),\r\n  },\r\n};\r\n</script>\r\n"]}]}